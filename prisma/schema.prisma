generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ShortUrl {
  id           Int      @id @default(autoincrement())
  shortUrl     String   @unique
  originalUrl  String   @unique
  createdBy    String?  
  createdAt    DateTime @default(now()) @db.Timestamptz(6)
  inactive     Int      @default(0)
}